{
  "name": "mala-ui",
  "version": "1.0.0",
  "description": "一个基于react18 + ts的前端UI组件库",
  "main": "index.js",
  "scripts": {
    "lint": "eslint components --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "build": "tsc --noEmit && rspack build",
    "build:analyze": "tsc --noEmit && rspack build --analyze",
    "commit": "git add . && cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@rspack/cli": "^0.1.6",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "clean-rspack-plugin": "^1.0.0",
    "commitizen": "4.2.4",
    "cz-customizable": "6.3.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "7.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2"
  }
}